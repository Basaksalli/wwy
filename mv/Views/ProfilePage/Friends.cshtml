@model List<mv.DataModel.Users>

<div style=" text-align: center;float:left" class="col-md-9">
    <h2>Friends</h2>
    @foreach (var item in Model)
    {
        <div  class="col-md-3">
            <img src="~/Content/images.jpg" class="img-circle" style="max-height:100px;max-width:100px" />
            <h3>@item.Name @item.Surname</h3>
        </div>
    }
</div>
